[masters]
k8s-cluster-viana-master-1 ansible_host=<MASTER_IP> ansible_user=<USER> ansible_ssh_private_key_file=<PRIVATE_KEY_PATH>

[workers]
k8s-cluster-viana-worker-1 ansible_host=<WORKER1_IP> ansible_user=<USER> ansible_ssh_private_key_file=<PRIVATE_KEY_PATH>
k8s-cluster-viana-worker-2 ansible_host=<WORKER2_IP> ansible_user=<USER> ansible_ssh_private_key_file=<PRIVATE_KEY_PATH>

[k8s_cluster:children]
masters
workers

[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'
ansible_python_interpreter=/usr/bin/python3
